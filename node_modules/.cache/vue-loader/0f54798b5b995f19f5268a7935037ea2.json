h1>\n    <input type=\"text\" name=\"username\" v-model=\"username\"/> <br>\n    <button @click=\"getUserData\"> Search </button>\n\n    <div v-if=\"user !== null\" class=\"profile\">\n      <img v-bind:src=\"user.avatar_url\" width=\"200\" height=\"200\"> <br>\n      <label>Username : {{ user.login }}</label> <br>\n      <label>Profile name : {{ user.name }}</label> <br>\n      <label>company : {{ user.company }}</label><br>\n       <label>create : {{ user.created_at }}</label>\n    </div>\n  </div>\n</template>\n\n<script>\n  import GithubService from '@/services/GithubService'\n  export default {\n    name: 'hello',\n    data () {\n      return {\n        msg: 'Fill your Github username for enter this workshop',\n        username: '',\n        user: null\n      }\n    },\n    methods: {\n      async getUserData() {\n        this.user = null;\n        const result = await GithubService.searchUser({\n          username: this.username\n        }).then(response => {\n          this.user = response.data\n        }).catch(error => {\n          console.log(error)\n        })\n        if(this.user !== null) {\n          console.log(this.user);\n        }\n      }\n    }\n  }\n</script>\n\n<style scoped>\n  h1, h2 {\n    font-weight: normal;\n  }\n  input {\n    width: 40%;\n    padding: 12px 20px;\n    margin: 8px 0;\n    display: inline-block;\n    border: 1px solid #ccc;\n    border-radius: 4px;\n    box-sizing: border-box;\n  }\n  button {\n    background-color: #4CAF50;\n    border: none;\n    color: white;\n    padding: 15px 32px;\n    text-align: center;\n    text-decoration: none;\n    display: inline-block;\n    font-size: 16px;\n  }\n  .profile {\n    padding: 16px;\n  }\n  label {\n    font-size: 20px;\n  }\n  \n</style>"]}]}